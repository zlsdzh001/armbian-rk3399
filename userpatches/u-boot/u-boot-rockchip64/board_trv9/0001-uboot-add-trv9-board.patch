Subject: [PATCH] uboot add rk3399 h339pc board

Signed-off-by: kaixinos <kaixinos@outlook.com>
---
 arch/arm/dts/Makefile                  |   1 +
 arch/arm/dts/rk3399-trv9-u-boot.dtsi   |  13 +
 arch/arm/dts/rk3399-trv9.dts           |  74 +++++++++++++++++++++++++
 configs/trv9-rk3399_defconfig          |  79 +++
 4 files changed, 167 insertions(+)
 create mode 100644 arch/arm/dts/rk3399-trv9-u-boot.dtsi
 create mode 100644 arch/arm/dts/rk3399-trv9.dts
 create mode 100644 configs/trv9-rk3399_defconfig

diff --git a/arch/arm/dts/Makefile b/arch/arm/dts/Makefile
index ee615e39..26235084 100644
--- a/arch/arm/dts/Makefile
+++ b/arch/arm/dts/Makefile
@@ -144,6 +144,7 @@ dtb-$(CONFIG_ROCKCHIP_RK3399) += \
 	rk3399-ficus.dtb \
 	rk3399-fine3399.dtb \
 	rk3399-firefly.dtb \
+	rk3399-trv9.dtb \
 	rk3399-gru-bob.dtb \
 	rk3399-gru-kevin.dtb \
 	rk3399-kobol-helios64.dtb \
diff --git a/arch/arm/dts/rk3399-trv9-u-boot.dtsi b/arch/arm/dts/rk3399-trv9-u-boot.dtsi
new file mode 100644
index 00000000..4b775f6d
--- /dev/null
+++ b/arch/arm/dts/rk3399-trv9-u-boot.dtsi
@@ -0,0 +1,13 @@
+// SPDX-License-Identifier: GPL-2.0+
+/*
+ * Copyright (C) 2019 Jagan Teki <jagan@amarulasolutions.com>
+ */
+
+#include "rk3399-u-boot.dtsi"
+#include "rk3399-sdram-lpddr4-100.dtsi"
+
+/ {
+	chosen {
+		u-boot,spl-boot-order = "same-as-spl", &sdhci, &sdmmc;
+	};
+};
diff --git a/arch/arm/dts/rk3399-trv9.dts b/arch/arm/dts/rk3399-trv9.dts
new file mode 100644
index 00000000..eac77f2a
--- /dev/null
+++ b/arch/arm/dts/rk3399-trv9.dts
@@ -0,0 +1,810 @@
+// SPDX-License-Identifier: (GPL-2.0+ OR MIT)
+/*
+ * Copyright (C) 2025 HPH
+ */
+
+/dts-v1/;
+#include "rk3399.dtsi"
+#include "rk3399-opp.dtsi"
+
+/ {
+	model = "HPH TRV-9";
+	compatible = "hph,trv-9", "rockchip,rk3399";
+
+	aliases {
+		ethernet0 = &gmac;
+		mmc0 = &sdio0;
+		mmc1 = &sdmmc;
+		mmc2 = &sdhci;
+	};
+
+	chosen {
+		stdout-path = "serial2:1500000n8";
+	};
+
+	/* 基本电源配置 */
+	vcc5v0_sys: vcc5v0-sys {
+		compatible = "regulator-fixed";
+		regulator-name = "vcc5v0_sys";
+		regulator-always-on;
+		regulator-boot-on;
+		regulator-min-microvolt = <5000000>;
+		regulator-max-microvolt = <5000000>;
+	};
+
+	vcc3v3_sys: vcc3v3-sys {
+		compatible = "regulator-fixed";
+		regulator-name = "vcc3v3_sys";
+		regulator-always-on;
+		regulator-boot-on;
+		regulator-min-microvolt = <3300000>;
+		regulator-max-microvolt = <3300000>;
+		vin-supply = <&vcc5v0_sys>;
+	};
+};
+
+/* 保留最基本的节点配置 */
+&cpu_l0 {
+	cpu-supply = <&vdd_cpu_l>;
+};
+
+&cpu_l1 {
+	cpu-supply = <&vdd_cpu_l>;
+};
+
+&cpu_l2 {
+	cpu-supply = <&vdd_cpu_l>;
+};
+
+&cpu_l3 {
+	cpu-supply = <&vdd_cpu_l>;
+};
+
+&cpu_b0 {
+	cpu-supply = <&vdd_cpu_b>;
+};
+
+&cpu_b1 {
+	cpu-supply = <&vdd_cpu_b>;
+};
+
+&gpu {
+	status = "okay";
+	mali-supply = <&vdd_gpu>;
+};
